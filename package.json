{
  "name": "taro-vue3-template",
  "version": "0.0.12",
  "private": true,
  "description": "demo",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp --env production",
    "build:swan": "taro build --type swan --env production",
    "build:alipay": "taro build --type alipay --env production",
    "build:tt": "taro build --type tt --env production",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn --env production",
    "build:qq": "taro build --type qq --env production",
    "build:jd": "taro build --type jd --env production",
    "build:quickapp": "taro build --type quickapp --env production",
    "dev": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .vue --ext .js --ext .ts src/",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "razzh",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/nutui-taro": "^3.2.1",
    "@tarojs/components": "^3.5.4",
    "@tarojs/helper": "^3.5.4",
    "@tarojs/plugin-framework-vue3": "^3.5.4",
    "@tarojs/plugin-html": "^3.5.4",
    "@tarojs/plugin-platform-alipay": "^3.5.4",
    "@tarojs/plugin-platform-jd": "^3.5.4",
    "@tarojs/plugin-platform-qq": "^3.5.4",
    "@tarojs/plugin-platform-swan": "^3.5.4",
    "@tarojs/plugin-platform-tt": "^3.5.4",
    "@tarojs/plugin-platform-weapp": "^3.5.4",
    "@tarojs/router": "^3.5.4",
    "@tarojs/runtime": "^3.5.4",
    "@tarojs/shared": "^3.5.4",
    "@tarojs/taro": "^3.5.4",
    "@tarojs/taro-h5": "^3.5.4",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@tarojs/cli": "^3.5.4",
    "@types/webpack-env": "^1.13.6",
    "@tarojs/webpack5-runner": "^3.5.4",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@unocss/webpack": "^0.45.12",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.5.4",
    "commitlint": "^17.0.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.5.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.4.0",
    "typescript": "^4.1.0",
    "unocss": "^0.45.12",
    "unocss-preset-weapp": "^0.1.11",
    "unplugin-transform-we-class": "^0.1.13",
    "vue-loader": "^16.0.0-beta.8",
    "vue-tsc": "^0.37.9",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
